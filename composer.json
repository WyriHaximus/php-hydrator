{
  "name": "wyrihaximus/hydrator",
  "description": "Middleware infused hydrator",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com",
      "homepage": "https://www.wyrihaximus.net/"
    }
  ],
  "require": {
    "php": "^7.4",
    "ocramius/generated-hydrator": "^4.0"
  },
  "require-dev": {
    "wyrihaximus/test-utilities": "^3.7"
  },
  "config": {
    "platform": {
      "php": "7.4.7"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\Hydrator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\Hydrator\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
